		<factor>0.1</factor>
		<offset>-0.5</offset>
		<min>-1.0</min>
		<max>1.5</max>
	    </pitch>
	    <position>
		<x> 12.7</x>
		<y> 2.34</y>
		<z> 1.37</z>
	    </position>
	    <orientation>
		<x>1</x>
		<y>0</y>
		<z>0</z>
		<inner-angle>90.0</inner-angle>
