	    <volume>
		<property>/engines/engine[0]/thrust_lb</property>
		<factor type="double">0.00006</factor>
	    </volume>
	    <pitch>
		<property>/engines/engine[0]/n1</property>
		<factor>0.20</factor>
	    </pitch>
	    <position>
		<x> 12.7</x>
