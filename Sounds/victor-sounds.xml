<?xml version="1.0"?>

<!--
    The coordinate system used is a left hand coordinate system where
    +Y = left, -Y = right, -Z = down, +Z = up, -X = forward, +X = aft.
    Distances are in metres.
-->

<PropertyList>

    <fx>

	<!-- ============================= External Power ============================= -->

	<!-- 
	    The GPU is parked underneath the port wing and is plugged into the aircraft just aft
	    of the bomb bay. The Palouste is parked underneath the starboard wing and the air supply
	    hose is connected to the aircraft just aft of the starboard landing gear.
	-->

	<!-- GPU start -->
	<gpu>
	    <name>GPUstart</name>
	    <path>Sounds/gpu-startup.wav</path>
	    <condition>
		<property>/controls/electric/external-power</property>
		<greater-than>
		    <property>/systems/electrical/bus/ground-ac-bus</property>
		    <value>0</value>
		</greater-than>
		<not>
		    <property>/sim/crashed</property>
		</not>
	    </condition>
	    <mode>once</mode>
	    <reference-dist>20</reference-dist>
	    <position>
		<x>10.0</x>
		<y>-1.0</y>
		<z>-1.0</z>
	    </position>
	    <volume>
		<factor>0.3</factor>
	    </volume>
	</gpu>

	<!-- GPU running -->
	<gpu>
	    <name>GPUrun</name>
	    <path>Sounds/gpu-running.wav</path>
	    <condition>
		<property>/controls/electric/external-power</property>
		<greater-than>
		    <property>/fdm/jsbsim/systems/electrical/gpu-generator</property>
		    <value>0.98</value>
		</greater-than>
		<not>
		    <property>/sim/crashed</property>
		</not>
	    </condition>
	    <mode>looped</mode>
	    <reference-dist>20</reference-dist>
	    <max-dist>100</max-dist>
	    <position>
		<x>10.00 </x>
		<y> 0.10 </y>
		<z>-0.15 </z>
	    </position>
	    <volume>
		<factor>0.3</factor>
	    </volume>
	</gpu>

	<!-- GPU shutdown -->
	<gpu>
	    <name>GPUstop</name>
	    <path>Sounds/gpu-running.wav</path>
	    <condition>
		<not>
		    <property>/controls/electric/external-power</property>
		</not>
		<not>
		    <property>/sim/crashed</property>
		</not>
	    </condition>
	    <mode>once</mode>
	    <reference-dist>20</reference-dist>
	    <max-dist>100</max-dist>
	    <position>
		<x>10.0</x>
		<y>-1.0</y>
		<z>-1.0</z>
	    </position>
	    <volume>
		<property>/fdm/jsbsim/systems/electrical/gpu-generator</property>
		<factor>0.3</factor>
	    </volume>
	</gpu>

	<!-- ============================= APU ============================= -->

	<!-- APU start-->
	<apu>
	    <name>APUstart</name>
	    <path>Sounds/apu-start.wav</path>
	    <condition>
		<equals>
		    <property>/controls/APU/off-start-run</property>
		    <value>1</value>
		</equals>
		<property>/engines/engine[4]/starter</property>
	    </condition>
	    <mode>once</mode>
	    <reference-dist>20</reference-dist>
	    <position>
		<x>12.70</x>
		<y>-0.76</y>
		<z> 0.76</z>
	    </position>
	    <orientation>
		<x> 0.98</x>
		<y> 0.0 </y>
		<z> 0.0 </z>
	    </orientation>
	    <volume>
		<factor>0.7</factor>
	    </volume>
	</apu>

	<!-- APU running -->
	<apu>
	    <name>APUrun</name>
	    <path>Sounds/apu-run.wav</path>
	    <condition>
		<equals>
		    <property>/controls/APU/off-start-run</property>
		    <value>2</value>
		</equals>
		<not>
		    <property>/sim/crashed</property>
		</not>
	    </condition>
	    <mode>looped</mode>
	    <reference-dist>20</reference-dist>
	    <max-dist>100</max-dist>
	    <volume>
		<property>/engines/engine[4]/n1</property>
		<factor>0.04</factor>
		<min>0.2</min>
	    </volume>
	    <pitch>
		<property>/engines/engine[4]/n2</property>
		<factor>0.1</factor>
		<offset>-0.5</offset>
		<min>-1.0</min>
		<max>1.5</max>
	    </pitch>
	    <position>
		<x>12.70</x>
		<y>-0.76</y>
		<z> 0.76</z>
	    </position>
	    <orientation>
		<x> 0.98</x>
		<y> 0.0 </y>
		<z> 0.0 </z>
	    </orientation>
	</apu>

	<!-- APU shutdown -->
	<apu>
	    <name>APUstop</name>
	    <path>Sounds/apu-stop.wav</path>
	    <condition>
		<and>
		    <not>
			<property>/controls/APU/run</property>
		    </not>
		    <greater-than>
			<property>/engines/engine[4]/n1</property>
			<value>1</value>
		    </greater-than>
		</and>
	    </condition>
	    <mode>once</mode>
	    <reference-dist>20</reference-dist>
	    <max-dist>100</max-dist>
	    <position>
		<x>12.70</x>
		<y>-0.76</y>
		<z> 0.76</z>
	    </position>
	    <orientation>
		<x> 0.98</x>
		<y> 0.0 </y>
		<z> 0.0 </z>
	    </orientation>
	    <volume>
		<factor>5</factor>
	    </volume>
	</apu>

