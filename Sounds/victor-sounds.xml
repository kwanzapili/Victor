	    <path>Sounds/aircraft-crash.wav</path>
	    <condition>
		<equals>
		    <property>/damage/sounds/crash-on</property>
		    <value>1</value>
		</equals>
	    </condition>
	    <mode>once</mode>
	    <position>
		<x>0.01</x>
		<y>0.01</y>
		<z>0.01</z>
	    </position>
	    <volume>
		<factor>1</factor>
	    </volume>
	</aircraft-crash>

	<aircraft-water-crash>
	    <name>aircraft-water-crash</name>
	    <path>Sounds/aircraft-water-crash.wav</path>
	    <condition>
		<equals>
		    <property>/damage/sounds/water-crash-on</property>
		    <value>1</value>
		</equals>
	    </condition>
	    <mode>once</mode>
	    <position>
		<x>0.01</x>
		<y>0.01</y>
		<z>0.01</z>
	    </position>
	    <volume>
		<factor>1</factor>
	    </volume>
	</aircraft-water-crash>

	<aircraft-crack>
	    <name>aircraft-crack</name>
	    <path>Sounds/aircraft-crack.wav</path>
	    <condition>
		<property>/sim/current-view/internal</property>
		<equals>
		    <property>/damage/sounds/crack-on</property>
		    <value>1</value>
		</equals>
	    </condition>
	    <mode>once</mode>
	    <position>
		<x>0.01</x>
		<y>0.01</y>
		<z>0.01</z>
	    </position>
	    <volume>
		<property>/damage/sounds/crack-volume</property>
		<factor>1</factor>
	    </volume>
	</aircraft-crack>

	<aircraft-creaking>
	    <name>aircraft-creaking</name>
	    <path>Sounds/aircraft-creaking.wav</path>
	    <condition>
		<property>/sim/current-view/internal</property>
		<equals>
		    <property>/damage/sounds/creaking-on</property>
		    <value>1</value>
		</equals>
	    </condition>
	    <mode>looped</mode>
	    <position>
		<x>0.01</x>
		<y>0.01</y>
		<z>0.01</z>
	    </position>
	    <volume>
		<property>/damage/sounds/creaking-volume</property>
		<factor>1</factor>
	    </volume>
	</aircraft-creaking>

	<!-- ============================= Ordinance ============================= -->

	<!-- Ordinance Explosion -->
	<explosion>
	    <name>bomb-explosion</name>
	    <path>Sounds/explosion.wav</path>
	    <condition>
		<property>/sim/sound/armament/explosion</property>
	    </condition>
	    <mode>looped</mode>
	    <volume>
		<property>/sim/sound/armament/explosion-volume</property>
		<factor>1</factor>
	    </volume>
	</explosion>

	<explosion>
	    <name>missile-hit</name>
	    <path>Sounds/missile-hit.wav</path>
	    <condition>
		<property>/sim/sound/armament/missile-hit</property>
	    </condition>
	    <mode>once</mode>
	    <volume>
		<property>/sim/sound/armament/explosion-volume</property>
		<factor>1</factor>
	    </volume>
	</explosion>

	<!-- Ordinance Separation from plane -->
	<seperation>
	    <name>detach-weapon</name>
	    <path>Sounds/armament-release.wav</path>
	    <condition>
		<property>/sim/sound/armament/armament-release</property>
	    </condition>
	    <mode>once</mode>
	    <pitch>
		<factor>0.7</factor>
		<offset>0.0</offset>
	    </pitch>
	    <volume>
		<factor>0.72</factor>
	    </volume>
	</seperation>

	<missile>
	    <name>shrike-fired</name>
	    <path>Sounds/missile.wav</path>
	    <condition>
		<property>/sim/sound/armament/missile-fired</property>
		<not><property>/sim/sound/armament/cruise-missile</property></not>
	    </condition>
	    <mode>once</mode>
	    <reference-dist>50</reference-dist>
	    <position>
		<x> 3.0 </x>
		<y> 0.0 </y>
		<z>-0.7 </z>
	    </position>
	    <volume>
		<property>/sim/sound/armament/missile-volume</property>
		<factor>1.0</factor>
	    </volume>
	</missile>

	<missile>
	    <name>cruise-missile-launch</name>
	    <path>Sounds/cruise-missile-launch.wav</path>
	    <condition>
		<property>/sim/sound/armament/missile-fired</property>
		<property>/sim/sound/armament/cruise-missile</property>
	    </condition>
	    <mode>once</mode>
	    <reference-dist>50</reference-dist>
	    <position>
		<x> 3.0 </x>
		<y> 0.0 </y>
		<z>-0.7 </z>
	    </position>
	    <volume>
		<property>/sim/sound/armament/missile-volume</property>
		<factor>1.0</factor>
	    </volume>
	</missile>

	<!-- ============================= Refuelling ============================= -->
	<refueling>
	    <name>proble-contact</name>
	    <path>Sounds/shlock.wav</path>
	    <condition>
		<property>/systems/refuel/contact</property>
	    </condition>
	    <mode>once</mode>
	    <position>
		<x>0.01</x>
		<y>0.01</y>
		<z>0.01</z>
	    </position>
	    <volume>
		<factor>1</factor>
	    </volume>
	</refueling>

    </fx>

</PropertyList>
