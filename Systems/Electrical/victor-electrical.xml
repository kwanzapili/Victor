    <connector>
	<name>APU Switch</name>
	<input>Distribution Bus AC</input>
	<input>Master Bus DC</input>
	<output>APU Starter</output>
	<switch>
	    <prop>/controls/engines/engine[4]/starter</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <!-- other engines need an air starter, but we use any source -->
    <connector>
	<name>Engine 1 starter</name>
	<input>Distribution Bus AC</input>
	<output>Starter 1 Power</output>
	<switch>
	    <prop>/controls/engines/engine[0]/starter</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <connector>
	<name>Engine 2 starter</name>
	<input>Distribution Bus AC</input>
	<output>Starter 2 Power</output>
	<switch>
	    <prop>/controls/engines/engine[1]/starter</prop>
	    <initial-state>off</initial-state>
	</switch>
