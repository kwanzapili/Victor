    <!--
	===========
	Connections
	===========
    -->

    <!--
	~~~~~~~~~~~~~~~~~~~~~
	Suppliers to buses
	~~~~~~~~~~~~~~~~~~~~~
    -->

    <!-- No. 1 battery energizes the No. 1 essential DC bus -->
    <connector>
	<input>Battery 1</input>
	<output>Essential Bus DC 1</output>
	<switch>
	    <prop>/controls/electric/battery-switch</prop>
	</switch>
    </connector>

    <!-- No. 2 battery energizes the No. 2 essential DC bus -->
    <connector>
	<input>Battery 2</input>
	<output>Essential Bus DC 2</output>
	<switch>
	    <prop>/controls/electric/battery-switch</prop>
	</switch>
    </connector>

    <!-- Ground power feeds is own bus which is then passed on to the main bus -->
    <connector>
	<input>Ground Power</input>
	<output>Ground Bus AC</output>
	<switch>
	    <prop>/controls/electric/external-power</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <!-- Reserve generator to its bus -->
    <connector>
	<input>Standby Power</input>
	<output>Standby Bus</output>
	<switch>
	    <prop>/fdm/jsbsim/systems/electrical/standby-power</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <!-- APU power -->
    <connector>
	<name>APU to its AC bus</name>
	<input>APU 200V</input>
	<output>APU Bus AC</output>
	<switch>
	    <prop>/controls/electric/engine[4]/generator</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <connector>
	<name>APU to AC Buses</name>
	<input>APU Bus AC</input>
	<output>Distribution Bus AC</output>
	<switch>
	    <prop>/controls/electric/engine[4]/bus-tie</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <!-- DC power from APU transformer -->
    <connector>
	<name>APU to DC Buses</name>
	<input>APU 28V</input>
	<output>APU Bus DC</output>
	<switch>
	    <prop>/controls/electric/APU-generator</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <!-- AC power from the engines -->

    <connector>
	<name>Conway1 to its Bus</name>
	<input>Conway1 200V</input>
	<output>ac_bus_1-1</output>
	<switch>
	    <prop>/controls/electric/engine[0]/generator</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <connector>
	<name>Conway2 to its Bus</name>
	<input>Conway2 200V</input>
	<output>ac_bus_1-2</output>
	<switch>
	    <prop>/controls/electric/engine[1]/generator</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <connector>
	<name>Conway3 to its Bus</name>
	<input>Conway3 200V</input>
	<output>ac_bus_2-3</output>
	<switch>
	    <prop>/controls/electric/engine[2]/generator</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <connector>
	<name>Conway4 to its Bus</name>
	<input>Conway4 200V</input>
	<output>ac_bus_2-4</output>
	<switch>
	    <prop>/controls/electric/engine[3]/generator</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <!-- Each engine can be individually connected to the master AC bus -->

    <connector>
	<name>Feed Distribution Bus</name>
	<input>ac_bus_1-1</input>
	<input>ac_bus_1-2</input>
	<input>ac_bus_2-3</input>
	<input>ac_bus_2-4</input>
	<input>Standby Bus</input>
	<input>Ground Bus AC</input>
	<output>Distribution Bus AC</output>
    </connector>

    <connector>
	<input>Distribution Bus AC</input>
	<output>Internal Lights Bus</output>
	<output>External Lights Bus</output>
	<switch>
	    <prop>/controls/electric/master-switch</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <!-- DC power from engines to DC buses -->
    <connector>
	<name>Conway1 to DC buses</name>
	<input>Conway1 28V</input>
	<output>dc_bus_left</output>
	<switch>
	    <prop>/controls/electric/engine[0]/bus-tie</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <connector>
	<name>Conway2 to DC buses </name>
	<input>Conway2 28V</input>
	<output>dc_bus_left</output>
	<switch>
	    <prop>/controls/electric/engine[1]/bus-tie</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <connector>
	<name>Conway3 to DC buses</name>
	<input>Conway3 28V</input>
	<output>dc_bus_right</output>
	<switch>
	    <prop>/controls/electric/engine[2]/bus-tie</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <connector>
	<name>Conway4 to DC buses</name>
	<input>Conway4 28V</input>
	<output>dc_bus_right</output>
	<switch>
	    <prop>/controls/electric/engine[3]/bus-tie</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <!--
	~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	Redistribution to secondary buses
	~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    -->

    <connector>
	<name>Feed Master DC Bus</name>
	<input>Essential Bus DC 1</input>
	<input>Essential Bus DC 2</input>
	<input>dc_bus_left</input>
	<input>dc_bus_right</input>
	<input>APU Bus DC</input>
	<output>Master Bus DC</output>
    </connector>

    <connector>
	<input>Master Bus DC</input>
	<output>Avionics Bus DC</output>
	<switch>
	    <prop>/controls/switches/master-avionics</prop>
	    <initial-state>off</initial-state>
	</switch>
	<switch>
	    <prop>/controls/electric/master-switch</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <connector>
	<input>Master Bus DC</input>
	<output>Lights Bus DC</output>
	<switch>
	    <prop>/controls/electric/master-switch</prop>
	    <initial-state>off</initial-state>
	</switch>
    </connector>

    <!--
	=========================
	Connect buses to outputs
	=========================
    -->

