<?xml version="1.0" encoding="utf-8"?>

<PropertyList>

    <!--
	==========
	Suppliers
	==========

The electrical power supplies required by the aircraft were divided into two basic systems:
* Medium Voltage 200V AC 3 Phase 400 Hertz and
* Low Voltage 28V DC.
To provide the LV requirements the output from the aircrafts alternators had to be transformed
and rectified. The alternators driven by each engine had an output of 200 volts AC frequency
400HZ and supplied power to the relevant MV and LV Bus Bars.
    -->

    <!-- Two 28 V batteries -->
    <supplier>
	<name>Battery 1</name>
	<prop>/systems/electrical/suppliers/battery[0]</prop>
	<kind>battery</kind>
	<volts>28</volts>
	<amps>25</amps>
    </supplier>

    <supplier>
	<name>Battery 2</name>
	<prop>/systems/electrical/suppliers/battery[1]</prop>
	<kind>battery</kind>
	<volts>28</volts>
	<amps>25</amps>
    </supplier>

    <!-- Reserve AC power (Ram Air turbine) for emergency use -->
    <supplier>
	<name>Standby Power</name>
	<prop>/systems/electrical/suppliers/standby</prop>
	<kind>alternator</kind>
	<rpm-source>/fdm/jsbsim/systems/electrical/generator-reserve-rpm</rpm-source>
	<rpm-threshold>500</rpm-threshold>
	<volts>200</volts>
	<amps>120</amps>
    </supplier>

    <!--
	APU Source: Rover gas turbine driving a 40kVA alternator
	Only active below an altitude of 30,000 ft (9,100 m)
    -->
    <supplier>
	<name>APU 200V</name>
	<prop>/systems/electrical/suppliers/apu</prop>
	<kind>alternator</kind>
	<rpm-source>/engines/engine[4]/n1</rpm-source>
	<rpm-threshold>30</rpm-threshold>
	<volts>200</volts>
	<amps>60</amps>
    </supplier>

    <supplier>
	<name>APU 28V</name>
	<prop>/systems/electrical/suppliers/apu-dc</prop>
	<kind>alternator</kind>	<!-- really a transformer rectifier from AC to DC -->
	<rpm-source>/systems/electrical/bus/apu-ac-bus</rpm-source>
	<rpm-threshold>100</rpm-threshold>
	<volts>28</volts>
	<amps>60</amps>
    </supplier>

    <!-- Engine Sources x 4 : 40 kVA three-phase 200 V AC at 400 Hz -->

    <!-- Engine 1 -->
    <supplier>
	<name>Conway1 200V</name>
	<prop>/systems/electrical/suppliers/alternator[0]</prop>
	<kind>alternator</kind>
	<rpm-source>/engines/engine[0]/n1</rpm-source>
	<rpm-threshold>30</rpm-threshold>
	<volts>200</volts>
	<amps>120</amps>
    </supplier>

    <supplier>
	<name>Conway1 28V</name>
	<prop>/systems/electrical/suppliers/tr[0]</prop>
	<kind>alternator</kind>	<!-- really a transformer rectifier from AC to DC -->
	<rpm-source>/systems/electrical/bus/ac-bus[0]</rpm-source>
	<rpm-threshold>100</rpm-threshold>
	<volts>28</volts>
	<amps>60</amps>
    </supplier>

    <!-- Engine 2 -->
    <supplier>
	<name>Conway2 200V</name>
	<prop>/systems/electrical/suppliers/alternator[1]</prop>
	<kind>alternator</kind>
	<rpm-source>/engines/engine[1]/n1</rpm-source>
	<rpm-threshold>30</rpm-threshold>
	<volts>200</volts>
	<amps>120</amps>
    </supplier>

    <supplier>
	<name>Conway2 28V</name>
	<prop>/systems/electrical/suppliers/tr[1]</prop>
	<kind>alternator</kind>	<!-- really a transformer rectifier from AC to DC -->
	<rpm-source>/systems/electrical/bus/ac-bus[1]</rpm-source>
	<rpm-threshold>100</rpm-threshold>
	<volts>28</volts>
	<amps>60</amps>
    </supplier>

    <!-- Engine 3 -->
    <supplier>
	<name>Conway3 200V</name>
	<prop>/systems/electrical/suppliers/alternator[2]</prop>
	<kind>alternator</kind>
	<rpm-source>/engines/engine[2]/n1</rpm-source>
	<rpm-threshold>30</rpm-threshold>
	<volts>200</volts>
	<amps>120</amps>
    </supplier>

    <supplier>
	<name>Conway3 28V</name>
	<prop>/systems/electrical/suppliers/tr[2]</prop>
	<kind>alternator</kind>	<!-- really a transformer rectifier from AC to DC -->
	<rpm-source>/systems/electrical/bus/ac-bus[2]</rpm-source>
	<rpm-threshold>100</rpm-threshold>
	<volts>28</volts>
	<amps>60</amps>
    </supplier>

    <!-- Engine 4 -->
    <supplier>
	<name>Conway4 200V</name>
	<prop>/systems/electrical/suppliers/alternator[3]</prop>
	<kind>alternator</kind>
	<rpm-source>/engines/engine[3]/n1</rpm-source>
	<rpm-threshold>30</rpm-threshold>
	<volts>200</volts>
	<amps>120</amps>
    </supplier>

    <supplier>
	<name>Conway4 28V</name>
	<prop>/systems/electrical/suppliers/tr[3]</prop>
	<kind>alternator</kind>	<!-- really a transformer rectifier from AC to DC -->
	<rpm-source>/systems/electrical/bus/ac-bus[3]</rpm-source>
	<rpm-threshold>100</rpm-threshold>
	<volts>28</volts>
	<amps>60</amps>
    </supplier>

    <!-- External power -->
    <supplier>
	<name>Ground Power</name>
	<prop>/systems/electrical/suppliers/external</prop>
	<kind>external</kind>
	<volts>115</volts> <!-- by default not connected -->
	<amps>90</amps>
    </supplier>

