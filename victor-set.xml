        <!--
            ======
            Views
            ======

            Notes:
            x-offset-m: Positive values to the right of origin (view position).
            y-offset-m: Positive values move up from origin.
            z-offset-m: Positive values move back (or in) from origin.
        -->

	<!-- position the pilot viewpoint and angle -->
	<view n="0">
	    <internal archive="y">true</internal>
	    <config>
		<x-offset-m archive="y">-0.51</x-offset-m>
		<y-offset-m archive="y"> 1.20</y-offset-m>
		<z-offset-m archive="y"> 3.75</z-offset-m>
		<pitch-offset-deg>-5.0</pitch-offset-deg>
	    </config>
	</view>

	<view n="1">
	    <config>
		<z-offset-m type="double" archive="y">-50</z-offset-m>
		<target-z-offset-m type="double" archive="y">15</target-z-offset-m>
		<default-field-of-view-deg type="double">60.0</default-field-of-view-deg>
	    </config>
	</view>

	<view n="2">
	    <config>
		<x-offset-m archive="y" type="double">8.0</x-offset-m>
		<target-z-offset-m type="double" archive="y">15</target-z-offset-m>
		<pitch-offset-deg>5.0</pitch-offset-deg>
	    </config>
	</view>

	<view n="3">
	    <config>
		<!-- big plane, so extend chase view offset a bit -->
		<z-offset-m type="double" archive="y">-80</z-offset-m>
		<target-z-offset-m type="double" archive="y">15</target-z-offset-m>
	    </config>
	</view>

	<view n="4">
	    <config>
		<target-z-offset-m type="double" archive="y">15</target-z-offset-m>
	    </config>
	</view>

	<!-- chase view without yaw -->
	<view n="5">
	    <config>
		<target-z-offset-m type="double" archive="y">15</target-z-offset-m>
	    </config>
	</view>

	<!-- fly-by view -->
	<view n="6">
	    <config>
		<target-z-offset-m type="double" archive="y">15</target-z-offset-m>
	    </config>
	</view>

	<!-- position the co-pilot viewpoint and angle -->
	<!-- This is definitely a dual-control a/c -->
	<view n="100">
	    <name>Co-pilot View</name>
	    <internal archive="y">true</internal>
	    <type>lookfrom</type>
	    <config>
		<from-model type="bool">true</from-model>
		<ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
		<x-offset-m archive="y">0.50</x-offset-m>
		<y-offset-m archive="y">1.00</y-offset-m>
		<z-offset-m archive="y">3.35</z-offset-m>
		<pitch-offset-deg>-5.0</pitch-offset-deg>
		<default-field-of-view-deg>75</default-field-of-view-deg>
		<dynamic-view type="bool">true</dynamic-view>
	    </config>
	</view>

	<view n="101">
	    <name>Panel View</name>
	    <internal archive="y">true</internal>
	    <type>lookfrom</type>
	    <config>
		<from-model type="bool">true</from-model>
		<x-offset-m archive="y">0.00</x-offset-m>
		<y-offset-m archive="y">1.00</y-offset-m>
		<z-offset-m archive="y">3.40</z-offset-m>
		<pitch-offset-deg>0.0</pitch-offset-deg>
		<default-field-of-view-deg type="double">75.0</default-field-of-view-deg>
	    </config>
	</view>

	<view n="102">
	    <name>Arm View</name>
	    <internal archive="y">true</internal>
	    <type>lookfrom</type>
	    <config>
		<from-model type="bool">true</from-model>
		<x-offset-m archive="y">-0.75</x-offset-m>
		<y-offset-m archive="y"> 1.00</y-offset-m>
		<z-offset-m archive="y"> 3.00</z-offset-m>
		<pitch-offset-deg>-70.0</pitch-offset-deg>
		<default-field-of-view-deg type="double">50.0</default-field-of-view-deg>
	    </config>
	</view>

