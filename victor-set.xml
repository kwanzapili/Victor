	<!-- Forward facing radar for terrain collision detection -->
	<air-ground-radar>

	    <serviceable type="bool">true</serviceable>
	    <!-- Modes are WX (weather) | WXA | MAP | TW (terrain) -->
	    <mode type="string">TW</mode>
	    <!-- For TW the "mode-control" needs to be 5 -->
	    <mode-control type="int">5</mode-control>
	    <!-- input properties not actually used internally by the instrument -->
	    <power-btn type="bool">false</power-btn>

	    <!--
		Antenna position configuration is done in the x-forward, y-right,
