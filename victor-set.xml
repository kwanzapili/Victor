	    <file>Aircraft/victor/Nasal/system.nas</file>
	    <file>Aircraft/victor/Nasal/ctrlpanel.nas</file>
	    <file>Aircraft/victor/Nasal/doors.nas</file>
	    <file>Aircraft/victor/Nasal/flightdirector.nas</file>
	    <file>Aircraft/victor/Nasal/fms.nas</file>
	    <file>Aircraft/victor/Nasal/Atmos.nas</file>
	</victor>

	<starter>
	    <file>Aircraft/victor/Nasal/starter.nas</file>
	</starter>

	<tyresmoke>
	    <file>Aircraft/victor/Nasal/tyresmoke.nas</file>
	</tyresmoke>

	<crash>
	    <file>Aircraft/victor/Nasal/crash.nas</file>
	</crash>

	<failures>
	    <file>Aircraft/Generic/limits.nas</file>
	    <file>Aircraft/victor/Nasal/failures.nas</file>
	</failures>

	<aar>
	    <file>Aircraft/Generic/aar.nas</file>
	</aar>

    </nasal>

    <!-- Aircraft limits -->
    <limits>

	<max-flap-extension-speed>
	    <flaps type="float">0.5</flaps>
	    <speed type="float">250</speed>
	</max-flap-extension-speed>
	<max-flap-extension-speed>
	    <flaps type="float">1.0</flaps>
	    <speed type="float">230</speed>
	</max-flap-extension-speed>

	<!-- Never exceed speed: 1,030 KPH / 640 MPH / 555 KT -->
        <vne type="int">555</vne>
        <!-- Maximum speed in level flight at maximum continuous power -->
        <mmo type="float">0.98</mmo>
        <!-- Stall speed or minimum steady flight speed. -->
        <Vstall-kt type="int">120</Vstall-kt>

	<max-gear-extension-speed type="int">235</max-gear-extension-speed>

	<max-positive-g type="float"> 4.0</max-positive-g>
	<max-negative-g type="float">-2.0</max-negative-g>

	<!-- maximum permissible vertical speed -->
	<max-vs-fps type="float">200</max-vs-fps>
	<!-- minimum, permissible vertical speed -->
	<min-vs-fps type="float">-80</min-vs-fps>

	<mass-and-balance>
	    <maximum-takeoff-mass-lbs type="float">  216000 </maximum-takeoff-mass-lbs>
	    <maximum-landing-mass-lbs type="float">  185000 </maximum-landing-mass-lbs>
	    <maximum-zero-fuel-mass-lbs type="float"> 90996 </maximum-zero-fuel-mass-lbs>
	</mass-and-balance>

    </limits>

    <!-- Default payload -->
    <payload>

	<!-- Crew: 5 men-->
	<weight n="0">
	    <name type="string">Crew</name>
	    <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[0]"/>
	    <min-lb type="double"> 350.0</min-lb>
	    <max-lb type="double">1250.0</max-lb>
	</weight>

	<!--
	    Equipment: based on mission - including fuel for tankers.
	    Armament are modelled separately so do not include that here.
	    The maximum payload is 51,000 lbs (equipment + armament).
	-->
	<weight n="1">
	    <name type="string">Equipment</name>
	    <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[1]"/>
	    <min-lb type="double"> 400 </min-lb>
	    <max-lb type="double">49600</max-lb>
	</weight>

    </payload>

    <!-- JSBSim Atmosphere model used instead of that from FG -->
    <environment>
	<!-- link the metar rain data with the base one -->
	<rain-norm alias="/environment/metar[0]/rain-norm"/>
	<snow-norm alias="/environment/metar[0]/snow-norm"/>

	<params>
	    <control-fdm-atmosphere type="bool">false</control-fdm-atmosphere>
	    <jsbsim-turbulence-model type="string">ttMilspec</jsbsim-turbulence-model>
	</params>
    </environment>

</PropertyList>
