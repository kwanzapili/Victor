    <!-- ~~~~~~~~~~~~ Speedbrake and Tail Chute Indicators ~~~~~~~~~~~~ -->

    <!-- 1/2 airbrakes on -->
    <animation>
	<type>material</type>
	<object-name>Circle.073</object-name>
	<condition include="Instruments/Lighting/led-powered.xml">
	    <greater-than>
		<property>/surface-positions/speedbrake-pos-norm</property>
		<value>0.05</value>
	    </greater-than>
	</condition>
	<emission>
	    <red>   0.7 </red>
	    <green> 0.2 </green>
	    <blue>  0.0 </blue>
	</emission>
    </animation>

