    <!-- Toggle trigger guard -->
    <animation>
	<type>pick</type>
	<object-name>Circle.040</object-name>
	<action>
	    <button>0</button>
	    <repeatable>false</repeatable>
	    <interval-sec>0.2</interval-sec>
	    <binding>
		<command>switch-toggle</command>
		<property>/controls/armament/trigger-guard</property>
	    </binding>
	</action>
	<hovered>
	    <binding>
		<command>set-tooltip</command>
		<label>Trigger Guard</label>
		<tooltip-id>trigger_guard</tooltip-id>
		<mapping>open-close</mapping>
		<property>/controls/armament/trigger-guard</property>
	    </binding>
	</hovered>
    </animation>
    <animation>
	<type>rotate</type>
	<object-name>Circle.040</object-name>
	<property>/controls/armament/trigger-guard</property>
	<factor> 160 </factor>
	<center>
	    <x-m> 0.005 </x-m>
	    <y-m>-0.1358 </y-m>
	    <z-m> 0.1547 </z-m>
	</center>
	<axis>
	    <x>  0 </x>
	    <y> -1 </y>
	    <z>  0 </z>
	</axis>
    </animation>

    <!-- Trigger -->
    <animation>
	<type>pick</type>
	<object-name>Circle</object-name>
	<action>
	    <button>0</button>
	    <repeatable>false</repeatable>
	    <interval-sec>0.2</interval-sec>
	    <binding>
