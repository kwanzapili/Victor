    <!-- ~~~~~~~ Station Selection and Trigger Operations ~~~~~~~ -->

    <animation>
	<name>bb_trigger</name>
	<object-name>Circle</object-name>
	<object-name>Circle.040</object-name>
	<object-name>Circle.041</object-name>
    </animation>

    <!-- Cycle through the different stations -->
    <animation>
	<type>knob</type>
	<object-name>Circle.041</object-name>
	<value>0.0</value>
	<center>
	    <x-m>0.0</x-m><y-m>0.0</y-m><z-m>0.0</z-m>
	</center>
	<axis>
	    <x>-1</x><y>0</y><z>0</z>
	</axis>

	<action>
	    <interval-sec>0.2</interval-sec>
	    <binding>
		<command>switch-adjust</command>
		<property>/controls/armament/station-select</property>
		<factor>1</factor>
		<min>0</min>
		<max>13</max>
		<wrap>true</wrap>
	    </binding>
	    <binding>
		<command>nasal</command>
		<script>victor.selectedStnStatus()</script>
	    </binding>
	</action>
	<hovered>
	    <binding>
		<command>set-tooltip</command>
		<label>Change Station</label>
		<tooltip-id>stn_select</tooltip-id>
	    </binding>
	</hovered>
    </animation>

    <!-- Bomb bay doors position -->
    <animation>
	<type>material</type>
	<object-name>Circle.039</object-name>
	<condition include="Instruments/Lighting/led-powered.xml"/>
	<emission>
	    <red>   0.7 </red>
	    <green> 0.2 </green>
	    <blue>  0.0 </blue>
	    <factor-prop>controls/doors/bomb-bay/position-norm</factor-prop>
