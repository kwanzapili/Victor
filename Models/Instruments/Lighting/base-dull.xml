<?xml version="1.0"?>

<!-- Base or inset of a switch / dial / knob : dull  -->

<PropertyList>

    <type>material</type>
    <condition>
	<property>controls/lighting/panel-lights</property>
	<!-- We must have enough electrical power -->
	<greater-than>
	    <property>systems/electrical/outputs/panel-lights</property>
	    <value> 100 </value>
	</greater-than>
    </condition>
    <emission>
	<factor-prop>controls/lighting/panel-norm</factor-prop>
	<red>   0.50 </red>
	<green> 0.35 </green>
	<blue>  0.20 </blue>
    </emission>
    <shininess> 20 </shininess>

</PropertyList>
