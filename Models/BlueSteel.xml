<?xml version="1.0"?>

<PropertyList>
    <path>BlueSteel.ac</path>  

    <animation>
	<type>select</type>
	<object-name>BlueSteel</object-name>
	<condition>
	    <greater-than>
		<property>/controls/armament/station[6]/units</property>
